@model IEnumerable<SimilatiryURLPartialViewModel>

<div>
    @if (Model.Count() > 0)
    {
        <div style="font-weight:500;color:red"> Was found some similars URL link in anothers Trackee that might mean it is repeated. Check it out!</div>
    }
    @foreach (var s in Model.OrderByDescending(e => e.similatiry))
    {
        <div style="background-color:@(s.similatiry==100?"#FD5C4C":s.similatiry>=90?"#DD7A00":s.similatiry>=80?"#3BA755":"white")">
            Similarity: @s.similatiry%  <a href="/Trackee/Edit/@s.TrackeeId"> @s.Tracker | @s.TrackeeId| @s.CompanyName | @s.JobTitle</a>
        </div>
    }
</div>
